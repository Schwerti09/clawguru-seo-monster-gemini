{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/((?!_next/static|_next/image|favicon.ico).*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/selfhealth/cron",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/launch/cron",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/agents/vulnerability",
      "schedule": "0 3 * * *"
    }
  ]
}
